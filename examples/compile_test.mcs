var bob = 3;
var chiller = 4;
var result = 2*(bob + chiller + 1);  // should be 16
var current_tick = 0;
var test_list = [1, "test", result];

function increment_result = (value) => {
    set result = result + value;
}

function increment_by_five = () => {
    increment_result(5);
    log(result);
}

function get_info = () => {
    log("current_tick: ", current_tick);
    log("test_list: ", test_list);
}

function kill = () => {
    log("Successfully uninstalled and disabled datapack.")
}

function main = () => {
    set current_tick = current_tick + 1;
    if (current_tick % 100 == 20) {
        log("1/5 seconds have passed");
    } else if (current_tick % 100 == 40) {
        log("2/5 seconds have passed");
    } else if (current_tick % 100 == 60) {
        log("3/5 seconds have passed");
    } else if (current_tick % 100 == 80) {
        log("4/5 seconds have passed");
    } else if (current_tick % 100 == 0) {
        log("5/5 seconds have passed");
    } else {
        log();
    }
}