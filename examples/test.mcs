function element_in = (value, list) => {
    for (element in list) {
        log(element);

        if (element == value) {
            return true;
        }
    }

    return false;
}

function recursive_loop = (current_loop, max_loop) => {
    if (current_loop > max_loop) {
        return;
    }

    log("current loop:", current_loop, "/", max_loop);
    recursive_loop(current_loop + 1, max_loop);
}

